{
  "scripts": {
    "debug:styles": "node bin/build-styles.js --sourcemap",
    "watch:styles": "node bin/build-styles.js --sourcemap --watch",
    "build:styles": "node bin/build-styles.js --sourcemap --minify",
    "debug:scripts": "tsc --project tsconfig.json",
    "watch:scripts": "tsc --watch --project tsconfig.json",
    "build:scripts": "tsc --project tsconfig.json && node bin/minify-app.js",
    "build:resources": "node bin/minify-resources",
    "build:app": "npm run build:styles && npm run build:scripts",
    "build:all": "npm run build:app && npm run build:resources"
  },
  "dependencies": {
    "@types/googlemaps": "^3.26.20",
    "@types/lodash": "^4.14.74",
    "@types/requirejs": "^2.1.31",
    "chalk": "^2.1.0",
    "cssnano": "^3.10.0",
    "js-yaml": "^3.9.1",
    "postcss": "^6.0.10",
    "postcss-cssnext": "^3.0.2",
    "postcss-discard-comments": "^2.0.4",
    "postcss-discard-empty": "^2.1.0",
    "postcss-import": "^10.0.0",
    "postcss-sorting": "^3.0.1",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2",
    "uglify-js": "^3.0.28",
    "vue": "^2.4.2",
    "vue-class-component": "^5.0.2",
    "vue-property-decorator": "^5.3.0",
    "vue-router": "^2.7.0",
    "vuex": "^2.4.0",
    "vuex-class": "^0.2.0"
  },
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  }
}
